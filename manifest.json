{
  "manifest_version": 2,

  "name": "WaterMe",
  "description": "Redirect your non productive site visits to organizations that reduce the global burden of disease.",
  "version": "1.0.1",

  "browser_action": {
   "default_icon": "images/icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "storage",
   "tabs"
  ],
"background": {
        "scripts": ["background.js"]
    },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"


}
